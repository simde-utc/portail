{
    "name": "simde-utc/portail-api",
    "description": "Portail des Assos de l'UTC.",
    "keywords": [
        "framework",
        "laravel",
        "simde",
        "api",
        "utc",
        "students",
        "clubs",
        "assos"
    ],
    "license": "GPL-v3",
    "type": "project",
    "require": {
        "php": ">=7.2",
        "cybercog/laravel-ownership": "^5.1",
        "diadal/passport": "^1.0@dev",
        "doctrine/dbal": "^2.6",
        "encore/laravel-admin": "^1.7",
        "fideloper/proxy": "~4.0",
        "grimzy/laravel-mysql-spatial": "^2.1",
        "ixudra/curl": "^6.16",
        "laravel-admin-ext/chartjs": "^1.0",
        "laravel-admin-ext/log-viewer": "^1.0",
        "laravel/framework": "5.8.*",
        "laravel/passport": "7.*",
        "laravel/tinker": "~1.0",
        "mews/captcha": "^2.2",
        "nastuzzi-samy/api-tester": "^1.1",
        "nastuzzi-samy/laravel-model-stages": "^1.0",
        "nastuzzi-samy/laravel-query-selection": "^2.5",
        "predis/predis": "^1.1",
        "webpatser/laravel-uuid": "^3.0"
    },
    "require-dev": {
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "1.*",
        "mockery/mockery": "~1.0",
        "nunomaduro/collision": "3.0.*",
        "phpunit/phpunit": "~8.0",
        "squizlabs/php_codesniffer": "3.*",
        "nunomaduro/larastan": "^0.3.17",
        "phpmd/phpmd": "^2.6"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "bootstrap/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ],
        "test": [
            "@composer install",
            "@php artisan portail:clear",
            "@php artisan portail:test"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
