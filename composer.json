{
    "name": "simde-utc/portail-api",
    "description": "Portail des Assos de l'UTC.",
    "keywords": [
        "framework",
        "laravel",
        "simde",
        "api",
        "utc",
        "students",
        "clubs",
        "assos"
    ],
    "license": "GPL-v3",
    "type": "project",
    "require": {
        "php": ">=7.2",
        "composer/composer": "*",
        "cybercog/laravel-ownership": "*",
        "diadal/passport": "*",
        "doctrine/dbal": "^2.0",
        "encore/laravel-admin": "^1.8",
        "fideloper/proxy": "*",
        "grimzy/laravel-mysql-spatial": "*",
        "ixudra/curl": "*",
        "laravel/framework": "7.*",
        "laravel/passport": "*",
        "laravel/tinker": "*",
        "mews/captcha": "*",
        "nastuzzi-samy/laravel-model-stages": "*",
        "nastuzzi-samy/laravel-query-selection": "*",
        "predis/predis": "*",
        "webpatser/laravel-uuid": "*"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "bootstrap/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ],
        "test": [
            "@composer install",
            "@php artisan portail:clear",
            "@php artisan portail:test"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
