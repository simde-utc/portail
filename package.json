		"redux": "^4.0.0",
		"babel-preset-react": "^6.23.0",
		"cross-env": "^5.2.0",
		"laravel-mix": "^2.1.11",
		"lodash": "^4.17.10",
		"semantic-ui-react": "^0.82.0",
